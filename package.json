{
  "name": "emays-ui",
  "version": "1.0.0",
  "description": "This is the Emays UI and BFF",
  "private": true,
  "scripts": {
    "dev-build": "tsc && env-cmd --no-override -e development vite build",
    "build": "tsc && env-cmd --no-override -e production vite build",
    "dev-start": "env-cmd --no-override -e development node ./app-dist/dev-server.js",
    "dev-mon": "env-cmd --no-override -e development nodemon --watch \"app/**\" --ext \"ts,json\" --exec \"ts-node\" dev-server.ts",
    "watch-script": "tsc && env-cmd --no-override -e development && yarn dev-start",
    "watch-ui": "vite build --watch",
    "watch-server": "nodemon --exec yarn watch-script",
    "start": "env-cmd --no-override -e production node ./app-dist/server.js",
    "lint": "eslint . --ext .ts --ext .jsx --ext .js ./public --quiet",
    "test": "jest",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "setupFiles": [
      "./test/setup.js"
    ]
  },
  "author": "",
  "license": "SEE LICENSE IN <filename>",
  "browserslist": "> 0.5%, last 2 versions, not dead",
