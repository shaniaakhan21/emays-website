{
  "name": "emays-ui",
  "version": "1.0.0",
  "description": "This is the Emays UI and BFF",
  "private": true,
  "scripts": {
    "dev-build": "tsc && env-cmd --no-override -e development vite build",
    "build": "tsc && env-cmd --no-override -e qa node vite build",
    "dev-start": "env-cmd --no-override -e development node ./app-dist/dev-server.js",
    "watch-script": "tsc && env-cmd --no-override -e development && yarn dev-start",
    "watch-ui": "vite build --watch",
    "watch-server": "nodemon --exec yarn watch-script",
    "start": "env-cmd --no-override -e qa node ./app-dist/server.js",
    "lint": "eslint . --ext .ts --ext .jsx --ext .js ./public --quiet",
    "test": "jest",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">= 14.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "verbose": true,
    "clearMocks": true,
    "collectCoverage": true,
    "automock": false,
    "setupFiles": [
      "./test/ui/setupJest.js"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": [
        "babel-jest",
        {
          "configFile": "./test/ui/.babelrc"
        }
      ],
      "^.+\\.(ts)$": "ts-jest",
      ".+\\.(css|styl|less|sass|scss|png|svg|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node",
      "jsx"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|svg|jpg|ttf|woff|woff2)$": "jest-transform-stub"
    }
  },
  "author": "",
  "license": "SEE LICENSE IN <filename>",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "dependencies": {
    "@aws-sdk/client-ses": "^3.282.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@carbon/react": "1.1.0",
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.3",
    "@testing-library/react": "12.1.2",
    "aws-sdk": "^2.1329.0",
    "axios": "^0.21.4",
    "babel-jest": "^27.5.1",
    "babel-plugin-styled-components": "^2.0.6",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "i18next": "^22.4.10",
    "jest": "^27.5.1",
    "joi": "^17.8.3",
    "json-web-token": "^3.2.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^6.5.4",
    "react": "^18.2.0",
    "react-datepicker": "^4.10.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.2.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.3.0",
    "react-test-renderer": "17.0.2",
    "sass": "1.51.0",
    "styled-components": "^5.3.3",
    "uuid": "^9.0.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@types/body-parser": "^1.19.2",
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^17.0.21",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-react": "^3.1.0",
    "chai": "^4.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^8.0.1",
    "jest-fetch-mock": "^3.0.3",
    "jest-sonar-reporter": "^2.0.0",
    "jest-specific-snapshot": "^5.0.0",
    "jest-transform-stub": "^2.0.0",
    "mocha": "^9.2.1",
    "nodemon": "^2.0.15",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "sinon": "^9.2.1",
    "ts-jest": "27.1.5",
    "typescript": "^4.6.2",
    "vite": "^4.1.0",
    "vite-plugin-static-copy": "^0.13.1"
  }
}
